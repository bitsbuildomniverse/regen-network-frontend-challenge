{
  "printWidth": 160,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketLine": false,
  "arrowParens": "always",
  "parser": "typescript",
  "requirePragma": false,
  "endOfLine": "auto",
  "singleAttributePerLine": false,
  "htmlWhitespaceSensitivity": "ignore"
}
